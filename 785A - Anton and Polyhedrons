n =  int(input())
cnt = 0;
while n:
    n -= 1
    a = input()
    if a == "Tetrahedron":
        cnt+=4
    if a == "Cube" :
        cnt+=6
    if a== "Octahedron":
        cnt+=8
    if a == "Dodecahedron":
        cnt += 12
    if a == "Icosahedron":
        cnt += 20
print(cnt)
